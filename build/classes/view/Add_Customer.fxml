<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Add_CustomerController">
   <children>
      <Button fx:id="saveBtn" layoutX="184.0" layoutY="348.0" mnemonicParsing="false" onAction="#onActionSaveBtn" prefHeight="25.0" prefWidth="61.0" text="Save" />
      <Label fx:id="addCustomerLabel" layoutX="172.0" layoutY="30.0" prefHeight="27.0" prefWidth="168.0" text="Add Customer" textAlignment="RIGHT">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="customerNameTxtBox" layoutX="76.0" layoutY="118.0" onAction="#onActionCustomerNameTxtBox" prefHeight="33.0" prefWidth="358.0" promptText="Customer Name" />
      <TextField fx:id="addressTxtBox" layoutX="76.0" layoutY="167.0" onAction="#onActionAddressTxtBox" prefHeight="33.0" prefWidth="358.0" promptText="Address" />
      <TextField fx:id="phoneNumberTxtBox" layoutX="266.0" layoutY="217.0" onAction="#onActionPhoneNumberTxtBox" prefHeight="33.0" prefWidth="168.0" promptText="Phone Number" />
      <TextField fx:id="postalCodeTxtBox" layoutX="77.0" layoutY="217.0" onAction="#onActionPostalCodeTxtBox" prefHeight="33.0" prefWidth="168.0" promptText="Postal Code" />
      <TextField fx:id="customerIdTxtBox" disable="true" layoutX="214.0" layoutY="72.0" onAction="#onActionCustomerIdTxtBox" prefHeight="33.0" prefWidth="83.0" promptText="Auto Gen ID" />
      <ComboBox fx:id="stateProvidenceComboBox" disable="true" layoutX="77.0" layoutY="289.0" prefHeight="33.0" prefWidth="168.0" promptText="Choose State" />
      <ComboBox fx:id="countryComboBox" layoutX="266.0" layoutY="289.0" onAction="#onActionCountryComboBox" prefHeight="33.0" prefWidth="168.0" promptText="Choose Country" />
      <Button fx:id="cancelBtn" layoutX="267.0" layoutY="348.0" mnemonicParsing="false" onAction="#onActionCancelBtn" prefHeight="25.0" prefWidth="61.0" text="Cancel" />
      <Label fx:id="customerIdLabel" layoutX="116.0" layoutY="80.0" prefHeight="17.0" prefWidth="72.0" text="Customer ID" />
      <Label fx:id="stateProvidenceLabel" layoutX="114.0" layoutY="267.0" prefHeight="17.0" prefWidth="94.0" text="State/Providence" />
      <Label fx:id="countryLabel" layoutX="303.0" layoutY="267.0" prefHeight="17.0" prefWidth="94.0" text="       Country" />
   </children>
</AnchorPane>
